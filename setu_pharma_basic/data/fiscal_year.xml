<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="generate_fiscal_year1" model="setu.pharma.fiscalyear">
            <field name="start_year" eval="datetime.now().strftime('%Y')"/>
            <field name="start_month">4</field>
            <field name="name" eval="'{} - {}'.format(datetime.now().year,
                                                   (datetime.now().year)+1)"/>
        </record>
        <record id="generate_fiscal_period1" model="setu.pharma.fiscalperiod">
            <field name="fiscalyear_id" ref="generate_fiscal_year1"/>
            <field name="start_date" eval="datetime.strptime('{}-{}-{}'.format(datetime.now().year, 4, '1'), '%Y-%m-%d')"/>
            <field name="end_date"  eval="datetime.strptime('{}-{}-{}'.format(datetime.now().year, 4, '30'), '%Y-%m-%d')"/>
            <field name="name">April</field>
        </record>
