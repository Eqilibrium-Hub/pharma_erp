<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="sales_incentive_action" model="ir.actions.act_window">
            <field name="name">Incentives</field>
            <field name="res_model">setu.pharma.incentive.structure</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="sales_target_action" model="ir.actions.act_window">
            <field name="name">Targets</field>
            <field name="res_model">setu.pharma.sales.target</field>
            <field name="view_mode">tree,form</field>
        </record>
        <record id="sales_target_lines_action" model="ir.actions.act_window">
            <field name="name">Target Product Lines</field>
            <field name="res_model">setu.pharma.target.product.lines</field>
            <field name="view_mode">tree</field>
        </record>
          <record id="sales_incentive_lines_action" model="ir.actions.act_window">
            <field name="name">Incentive Lines</field>
            <field name="res_model">setu.pharma.incentive.structure.lines</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="sales_target_menu"
                  name="Sales Targets"
                  action="sales_incentive_action"
                  parent="sale.sale_menu_root"
                  sequence="6"/>

          <menuitem id="target_menu"
                  name="Targets"
                  parent="sales_target_menu"
                  action="sales_target_action"
                  sequence="1"/>
          <menuitem id="incentive_menu"
                  name="Incentive"
                  parent="sales_target_menu"
                  action="sales_incentive_action"
                  sequence="2"/>

    </data>
